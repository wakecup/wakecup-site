    "@unform/web": "^2.1.6",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.1.2",
    "gatsby-image": "^3.1.0",
    "gatsby-plugin-catch-links": "^3.1.0",
    "gatsby-plugin-google-analytics": "^3.1.0",
    "gatsby-plugin-google-tagmanager": "^3.1.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^1.1.2",
    "gatsby-plugin-lodash": "^4.1.0",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.1.2",
    "gatsby-plugin-sitemap": "^3.1.0",
    "gatsby-plugin-styled-components": "^4.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-images": "^4.1.1",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-transformer-remark": "^3.1.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "polished": "^4.1.1",
    "ptz-i18n": "^1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-intl": "^5.13.5",
    "react-reveal": "^1.2.2",
