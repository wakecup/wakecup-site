{
  "name": "wakecup-website",
  "private": true,
  "version": "2.0.0",
  "author": "Gabriel Maia",
  "keywords": [
    "react",
    "gatsby",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "gatsby develop -o",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint": "eslint --ext .js --ext .jsx ./src/",
    "lint:styles": "stylelint \"src/**/*.ts\" -s css-in-js --config .stylelintrc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:styles",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
