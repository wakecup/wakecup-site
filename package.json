{
  "name": "wakecup-website",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and styled-system",
  "version": "2.0.0",
  "author": "Gregor Albrecht <hello@gregoralbrecht.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled"
  },
  "bugs": {
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled/issues"
  },
  "keywords": [
    "react",
    "gatsby",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "gatsby develop -o",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint": "eslint --ext .js --ext .jsx ./src/",
    "lint:styles": "stylelint \"src/**/*.ts\" -s css-in-js --config .stylelintrc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:styles",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-catch-links": "^2.3.12",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-lodash": "^3.3.11",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-plugin-sitemap": "^2.4.13",
    "gatsby-plugin-styled-components": "^3.3.11",
    "gatsby-plugin-typescript": "^2.4.20",
    "gatsby-plugin-typography": "^2.5.11",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-images": "^3.3.30",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-remark": "^2.8.35",
    "gatsby-transformer-sharp": "^2.5.15",
    "polished": "^3.6.7",
    "ptz-i18n": "^1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-intl": "^5.8.2",
    "react-reveal": "^1.2.2",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-scroll": "^1.8.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.3"
  }
}
